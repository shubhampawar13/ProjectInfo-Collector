{
    "_version": "1.26.0",
    "sap.app": {
        "id": "projectInfoColl.workflowuimodule",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap/generator-fiori:basic",
            "version": "1.8.4",
            "toolsId": "4f288adb-65d7-497f-ad83-8573dd6cb2ea"
        },
        "dataSources": {
            "mainService": {
                "uri": "/sap/opu/odata/",
                "type": "OData",
                "settings": {
                    "annotations": [],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "2.0"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.82.2",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.f": {},
                "sap.suite.ui.generic.template": {},
                "sap.ui.comp": {},
                "sap.ui.generic.app": {},
                "sap.ui.table": {},
                "sap.ushell": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "projectInfoColl.workflowuimodule.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "async": true,
                "viewPath": "projectInfoColl.workflowuimodule.view",
                "controlAggregation": "pages",
                "controlId": "app",
                "clearControlAggregation": false
            },
            "routes": [
                {
                    "name": "RouteProjectInfoCollector",
                    "pattern": ":?query:",
                    "target": [
                        "TargetProjectInfoCollector"
                    ]
                }
            ],
            "targets": {
                "TargetProjectInfoCollector": {
                    "viewType": "XML",
                    "transition": "slide",
                    "clearControlAggregation": false,
                    "viewId": "ProjectInfoCollector",
                    "viewName": "ProjectInfoCollector"
                }
            }
        },
        "rootView": {
            "viewName": "projectInfoColl.workflowuimodule.view.App",
            "type": "XML",
            "async": true,
            "id": "App"
        }
    },
    "sap.cloud": {
        "public": true,
        "service": "projectInfoCollectorUi"
    },
    "sap.bpa.task": {
        "_version": "1.0.0",
        "outcomes": [
            {
                "id": "approve",
                "label": "Approve"
            },
            {
                "id": "reject",
                "label": "Reject"
            }
        ],
        "inputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "input",
            "type": "object",
            "required": [
                "empId",
                "empName",
                "endingDate",
                "projectName",
                "skills",
                "startingDate",
                "status"
            ],
            "properties": {
                "empId": {
                    "type": "string",
                    "title": "empId",
                    "description": "empId"
                },
                "empName": {
                    "type": "string",
                    "title": "empName",
                    "description": "EmployeeName"
                },
                "endingDate": {
                    "type": "string",
                    "title": "endingDate",
                    "description": "EndingDate"
                },
                "projectName": {
                    "type": "string",
                    "title": "projectName",
                    "description": "ProjectName"
                },
                "skills": {
                    "type": "array",
                    "title": "skills",
                    "description": "skills"
                },
                "startingDate": {
                    "type": "string",
                    "title": "startingDate",
                    "description": "StartingDate"
                }
                ,
                "status": {
                    "type": "string",
                    "title": "status",
                    "description": "Billingstatus"
                }
            }
        },
        "outputs": {
            "$schema": "http://json-schema.org/draft-07/schema",
            "title": "output",
            "type": "object",
            "required": [
                "comment"
            ],
            "properties": {
                "comment": {
                    "type": "string",
                    "title": "Comment",
                    "description": "Comment to buyer"
                }
            }
        },
        "category": "approval"
    }

}